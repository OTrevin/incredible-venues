<p id="notice"><%= notice %></p>

<h1>All venues</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Address</th>
      <th>Capacity</th>


      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @venues.each do |venue| %>
      <tr>
        <td><%= venue.name %></td>
        <td><%= venue.address %></td>
        <td><%= venue.capacity %></td>
        <td><%= link_to 'Show', venue %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>


